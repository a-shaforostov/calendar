{
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "env": {
    "browser": true,
    "jquery": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "linebreak-style": "off",
    "require-jsdoc": "off",
    "padded-blocks": "off",
    "no-unused-vars": ["error", { "vars": "local" }],
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
    "no-console": 1,
    "no-debugger": 1,
    "max-len": [1, 120],
    "no-invalid-this": 1
  },
  "externals": ["$", "_", "Handlebars"]
}